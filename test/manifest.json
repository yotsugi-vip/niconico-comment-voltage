{
    "name": "nico2CommentVoltage",
    "version": "0.0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://www.nicovideo.jp/watch/*"
            ],
            "js": [
                "jquery.js",
                "index.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "back.js"
        ],
        "persitent": false
    },
    "permissions": [
        "webRequest",
        "*://*.google.com/"
    ]
}